{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "keyVaultAdmin": {
            "type": "string",
            "metadata": {
                "description": "The object ID of the user or group who will have full permissons on Azure Key Vault"
            }
        },
        "onPremisesAddressSpace": {
            "type": "string",
            "metadata": {
                "description": "The summarized address space of any on-premises locations"
            },
            "defaultValue": "192.168.0.0/16"
        },
        "primaryLocation": {
            "type": "string",
            "metadata": {
                "description": "The primary Azure region to deploy resources to."
            },
            "allowedValues": [
                "centralus",
                "eastus",
                "eastus2",
                "southcentralus",
                "westus",
                "westus2"
            ]
        },
        "secondaryLocation": {
            "type": "string",
            "metadata": {
                "description": "The secondary Azure region to deploy resources to."
            },
            "allowedValues": [
                "centralus",
                "eastus",
                "eastus2",
                "southcentralus",
                "westus",
                "westus2"
            ]
        },
        "tags": {
            "type": "object",
            "metadata": {
                "description": "The tags that wil be associated to the resources"
            },
            "defaultValue": {
                "environment": "lab"
            }
        },
        "transitSubscriptionId": {
            "type": "string",
            "metadata": {
                "description": "The id of the subscription where the Transit resource group and resources will be deployed"
            },
            "defaultValue": "[subscription().subscriptionId]"
        },
        "uniqueData": {
            "type": "string",
            "metadata": {
                "description": "Creates a new GUID to create uniqueness for resources"
            },
            "defaultValue": "[substring(newGuid(),0,8)]"
        },
        "sharedSvcSubscriptionId": {
            "type": "string",
            "metadata": {
                "description": "The id of the subscription where the Shared Services resource group and resources will be deployed"
            },
            "defaultValue": "[subscription().subscriptionId]"
        },
        "workloadSubscriptionId": {
            "type": "string",
            "metadata": {
                "description": "The id of the subscription where the Workload resource group and resources will be deployed"
            },
            "defaultValue": "[subscription().subscriptionId]"
        },
        "vmAdminUsername": {
            "type": "string",
            "metadata": {
                "description": "Administrator name for VMs that are created"
            }
        },
        "vmAdminPassword": {
            "type": "securestring",
            "metadata": {
                "description": "Password for the VMs that are created"
            }
        }
    },
    "variables": {
        "_apiVersionResources": "2020-06-01",
        "addressSpaceAzure": "10.0.0.0/8",
        "addressSpaceAzureR1": "10.0.0.0/13",
        "addressSpaceAzureR2": "10.8.0.0/13",
        "rgDeploymentNameDns": "[concat('deploy-dns',parameters('uniqueData'))]",
        "rgDeploymentNameMediationR1": "[concat('deploy-medr1',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameMediationR2": "[concat('deploy-medr2',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameSharedSvcR1": "[concat('deploy-ssr1',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameSharedSvcR2": "[concat('deploy-ssr2',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameVwan": "[concat('deploy-vwan',parameters('uniqueData'))]",
        "rgDeploymentNameVwanHubR1": "[concat('deploy-vwanhr1',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameVwanHubR2": "[concat('deploy-vwanhr2',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameWorkloadR1": "[concat('deploy-wlr1',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgDeploymentNameWorkloadR2": "[concat('deploy-wlr2',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "rgNameTransitR1": "[concat('rg-tr-',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgNameTransitR2": "[concat('rg-tr-',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "rgNameSharedSvcR1": "[concat('rg-ss-',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgNameSharedSvcR2": "[concat('rg-ss-',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "rgNameWorkloadR1": "[concat('rg-wl-',parameters('primaryLocation'),parameters('uniqueData'))]",
        "rgNameWorkloadR2": "[concat('rg-wl-',parameters('secondaryLocation'),parameters('uniqueData'))]",
        "subDeploymentNameTransitR1": "[concat('deploy-trr1-sub',parameters('uniqueData'))]",
        "subDeploymentNameTransitR2": "[concat('deploy-trr2-sub',parameters('uniqueData'))]",
        "subDeploymentNameSharedSvcR1": "[concat('deploy-ssr1-sub',parameters('uniqueData'))]",
        "subDeploymentNameSharedSvcR2": "[concat('deploy-ssr2-sub',parameters('uniqueData'))]",
        "subDeploymentNameWorkloadR1": "[concat('deploy-wlr1-sub',parameters('uniqueData'))]",
        "subDeploymentNameWorkloadR2": "[concat('deploy-wlr2-sub',parameters('uniqueData'))]",
        "templateFileDns": "[concat(variables('templateUriSharedSvc'), 'deploy-dns.json')]",
        "templateFileMediation": "[concat(variables('templateUriTransit'), 'deploy-mediation.json')]",
        "templateFileSharedSvc": "[concat(variables('templateUriSharedSvc'), 'deploy-shared.json')]",
        "templateFileVwan": "[concat(variables('templateUriTransit'), 'deploy-vwan.json')]",
        "templateFileVwanHub": "[concat(variables('templateUriTransit'), 'deploy-vwan-hub.json')]",
        "templateFileWorkload": "[concat(variables('templateUriWorkload'), 'deploy-workload.json')]",
        "templateUriSharedSvc": "[concat(uri(deployment().properties.templateLink.uri,'templates/core/shared/'))]",
        "templateUriTransit": "[concat(uri(deployment().properties.templateLink.uri,'templates/core/transit/'))]",
        "templateUriWorkload": "[concat(uri(deployment().properties.templateLink.uri,'templates/core/workload/'))]",
        "vmServerIpLinuxDev": "10.2.0.10",
        "vmServerIpWindowsDev": "10.2.0.20",
        "vnetCidrTransitR1": "10.1.0.0/16",
        "vnetCidrTransitR2": "10.9.0.0/16",
        "vnetCidrSharedSvcR1": "10.2.0.0/16",
        "vnetCidrSharedSvcR2": "10.10.0.0/16",
        "vnetCidrWorkloadR1": "10.3.0.0/16",
        "vnetCidrWorkloadR2": "10.11.0.0/16",
        "vnetSubnetCidrSharedSvcDevR1": "10.2.0.0/24",
        "vnetSubnetCidrSharedSvcDevR2": "10.10.0.0/24",
        "vnetSubnetCidrSharedSvcDnsInR1": "10.2.1.0/28",
        "vnetSubnetCidrSharedSvcDnsInR2": "10.10.1.0/28",
        "vnetSubnetCidrSharedSvcDnsOutR1": "10.2.2.0/28",
        "vnetSubnetCidrSharedSvcDnsOutR2": "10.10.2.0/28",
        "vnetSubnetCidrSharedSvcSvcR1": "10.2.3.0/24",
        "vnetSubnetCidrSharedSvcSvcR2": "10.10.3.0/24",
        "vnetSubnetCidrTransitAzFwR1": "10.1.0.0/24",
        "vnetSubnetCidrTransitAzFwR2": "10.9.0.0/24",
        "vnetSubnetCidrTransitBastionR1": "10.1.1.0/27",
        "vnetSubnetCidrTransitBastionR2": "10.9.1.0/27",
        "vnetSubnetCidrWorkloadAppR1": "10.3.0.0/24",
        "vnetSubnetCidrWorkloadAppR2": "10.3.0.0/24",
        "vnetSubnetCidrWorkloadDataR1": "10.3.1.0/24",
        "vnetSubnetCidrWorkloadDataR2": "10.11.1.0/24",
        "vnetSubnetCidrWorkloadSvcR2": "10.3.2.0/24",
        "vnetSubnetCidrWorkloadSvcR1": "10.3.2.0/24",
        "vwanHubCidrR1": "10.0.0.0/16",
        "vwanHubCidrR2": "10.8.0.0/16"
    },
    "resources": [
        {
            "name": "[variables('subDeploymentNameTransitR1')]",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "[variables('_apiVersionresources')]",
            "location": "[parameters('primaryLocation')]",
            "subscriptionId": "[parameters('transitSubscriptionId')]",
            "properties": {
                "mode": "Incremental",
